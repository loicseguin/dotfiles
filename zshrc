alias ls='ls --color'

# Activate main Python environment
alias mainpy='source ${HOME}/.mainpy/bin/activate'

# Goto places
alias dotf='cd ${HOME}/code/dotfiles'
alias nyb='cd ${HOME}/enseignement/203-nyb-electricite-magnetisme'
alias sn3='cd ${HOME}/enseignement/203-sn3-ondes'

# Git
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gp='git push'
alias gpl='git pull'
alias gco='git checkout'
